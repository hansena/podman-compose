{
	"extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings", "prettier"],
	"plugins": ["babel", "prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": false,
		"node": true,
		"jquery": false,
		"jest": true
	},
	"rules": {
		"import/no-unresolved": 0,
		"quotes": [1, "double", { "avoidEscape": true, "allowTemplateLiterals": true }],
		"no-console": 0,
		"no-debugger": 1,
		"no-var": 1,
		"semi": [1, "always"],
		"no-trailing-spaces": 1,
		"eol-last": 1,
		"no-unused-vars": [2, { "ignoreRestSiblings": true }],
		"no-underscore-dangle": 0,
		"no-alert": 0,
		"no-lone-blocks": 2,
		"arrow-parens": 1,
		"prettier/prettier": 2,
		"require-atomic-updates": 0
	}
}
